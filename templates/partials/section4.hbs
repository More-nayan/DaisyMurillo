<section class="fourthSec" id="section_four">
      <div class="contact">
        <div class="content">
          <p><span class="line bColor"></span></p>
          <h1 class="wc main_name">
            Contacts
            <span class="arrow"
              ><img src="./images/arrow-l-b.png" alt="arrow"
            /></span>
          </h1>
        </div>
        <div class="social_icons">
          <i class="fab fa-instagram"></i>
          <i class="fab fa-twitter"></i>
          <i class="fab fa-tiktok"></i>
          <i class="fab fa-discord"></i>
          <i class="fab fa-github"></i>
        </div>
      </div>

      <form action="/register" method="POST">
        <div class="form_input mr">
        <label for="fname">First Name</label>
        <input type="text" id="fname" name="firstname" placeholder="First Name">
      </div>
        <div class="form_input">
        <label for="lname">Last Name</label>
        <input type="text" id="lname" name="lastname" placeholder="Last Name">
      </div>
        <div class="form_input mr">
        <label for="email">Email</label>
        <input type="email" id="email" name="email" placeholder="Email">
      </div>
        <div class="form_input">
        <label for="phone">Phone</label>
        <input type="phone" id="phone" name="phone" placeholder="Phone">
      </div>

      <div>
        <label for="message">Message</label>
        <textarea name="message" id="message" cols="" rows=""></textarea>
      </div>
      <input type="submit" value="Submit">
      </form>
    </section>